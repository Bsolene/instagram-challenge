<div class="navbar-insta">

  <a href="/" class="navbar-insta-brand">
    <%= image_tag("logo.png") %>
  </a>

  <div class="navbar-insta-right">
    <% if user_signed_in? %>
      <div class="navbar-insta-item navbar-insta-link">
        <%= link_to destroy_user_session_path, class: "", method: :delete do %>
          <span>logout</span>
        <% end %>
      </div>
    <% else %>
      <div class="navbar-insta-item navbar-insta-link">
        <%= link_to new_user_registration_path, class: "" do %>
          <span>sign up</span>
        <% end %>
      </div>
      <div class="navbar-insta-item navbar-insta-link">
        <%= link_to user_session_path, class: "" do %>
          <span>login</span>
        <% end %>
      </div>

    <% end %>
  </div>

</div>